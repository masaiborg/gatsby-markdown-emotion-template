{
  "name": "gatsby-starter-emotion-typescript",
  "version": "1.0.0",
  "description": "Blog template for Gatsby.",
  "keywords": [
    "gatsby",
    "typescript",
    "emotion",
    "cms"
  ],
  "author": "Masaki Yoshiiwa <yopinoji@gmail.com>",
  "license": "MIT",
  "config": {
    "port": 8000
  },
  "scripts": {
    "dev": "npm run develop",
    "check": "npm run type-check && npm run stylelint && npm run eslint && npm run prettier",
    "type-check": "tsc --noEmit",
    "prettier": "prettier . --write",
    "eslint": "eslint . --ext ts,tsx --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,sass,ts,tsx,js,jsx}\" --fix",
    "build": "gatsby build",
    "develop": "gatsby develop --port $npm_package_config_port",
    "develop-in-container": "gatsby develop --host 0.0.0.0 --port $npm_package_config_port",
    "serve": "gatsby build && gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/YopiNoji/gatsby-js-blog/issues"
  },
  "homepage": "https://github.com/YopiNoji/gatsby-js-blog#readme",
  "dependencies": {
    "@emotion/react": "11.1.5",
    "gatsby": "2.32.8",
    "gatsby-image": "2.11.0",
    "gatsby-plugin-emotion": "5.4.0",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-feed": "2.13.0",
    "gatsby-plugin-google-analytics": "2.11.0",
    "gatsby-plugin-graphql-codegen": "2.7.1",
    "gatsby-plugin-manifest": "2.12.0",
    "gatsby-plugin-offline": "3.10.1",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-sharp": "2.14.1",
    "gatsby-plugin-tsconfig-paths": "1.0.2",
    "gatsby-plugin-typescript": "2.12.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.21",
    "gatsby-remark-autolink-headers": "2.11.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.11.0",
    "gatsby-remark-prismjs": "3.13.0",
    "gatsby-source-filesystem": "2.11.0",
    "gatsby-transformer-remark": "2.16.0",
    "gatsby-transformer-sharp": "2.12.0",
    "prismjs": "1.23.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-spring": "9.0.0"
  },
  "devDependencies": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.3",
    "@types/react-helmet": "6.1.0",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "eslint": "7.22.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.2.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "stylelint": "13.12.0",
    "stylelint-config-standard": "20.0.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,scss,sass,ts,tsx,js,jsx}": [
      "npm run check"
    ]
  }
}
